<?xml version="1.0" encoding="utf-8"?>
<identity xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    name="SecureBoot"
    namespace="Security"
    owner="$(OEMNAME)"
    legacyName="$(OEMNAME).Security.SecureBoot" xmlns="urn:Microsoft.CompPlat/ManifestSchema.v1.00">
    <onecorePackageInfo
        targetPartition="MainOS"
        releaseType="Production"
        ownerType="OEM" />
    <files>
        <file
            destinationDir="$(runtime.bootDrive)\IoTSec"
            source="SetVariable_db.bin" />
        <file
            destinationDir="$(runtime.bootDrive)\IoTSec"
            source="SetVariable_kek.bin" />
        <file
            destinationDir="$(runtime.bootDrive)\IoTSec"
            source="SetVariable_pk.bin" />
        <file
            destinationDir="$(runtime.bootDrive)\IoTSec"
            source="setup.secureboot.cmd" />
    </files>
</identity>